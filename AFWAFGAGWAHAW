-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
local venyx = library.new("Wierdo Hub; ESP UI", 5013109572)

local Aiming = loadstring(game:HttpGet("https://raw.githubusercontent.com/Pi-314Git/CreditsToStefanuk12/main/ModuleF.lua"))()
local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()
ESP:Toggle(false)
ESP.Tracers = false
ESP.Names = false
ESP.Boxes = false
Aiming.TeamCheck(false)
Aiming.VisibleCheck = false
-- // Dependencies

-- // Services
local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

-- // Vars
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local CurrentCamera = Workspace.CurrentCamera

local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

local DaHoodSettings = {
    SilentAim = false,
    AimLock = false,
    Prediction = 0.165,
}

getgenv().DaHoodSettings = DaHoodSettings

-- first page
local page = venyx:addPage("Esp", 5012544693)
local esptab = page:addSection("ESP INFORMATION!")

esptab:addToggle("Enable ESP", nil, function(value)
    ESP:Toggle(value)
end)

esptab:addToggle("Enable Tracers", nil, function(value)
    ESP.Tracers = value
end)

esptab:addToggle("Enable Names", nil, function(value)
    ESP.Names = value
end)

esptab:addToggle("Enable Boxes", nil, function(value)
    ESP.Boxes = value
end)


-- first page
local page = venyx:addPage("Credits", 5012544693)
local credits = page:addSection("Developers :")

credits:addButton("made by: slump#3267")

local page = venyx:addPage("Ui Toggle", 5012544693)
local bind = page:addSection("Settings :")

bind:addKeybind("Toggle Keybind", Enum.KeyCode.One, function()
venyx:toggle()
end, function()
end)
